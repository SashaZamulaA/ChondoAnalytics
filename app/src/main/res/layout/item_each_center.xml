<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/cardView2"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginStart="8dp"
    android:layout_marginTop="2dp"
    android:layout_marginBottom="2dp"
    android:elevation="2dp"
    android:layout_marginEnd="8dp"
    app:cardBackgroundColor="@color/white">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ImageView
            android:id="@+id/imageView_profile_picture"
            android:layout_width="86dp"
            android:layout_height="86dp"
            android:layout_marginTop="24dp"
            android:layout_marginBottom="8dp"
            android:padding="@dimen/space_8"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/ic_account_circle_black_24dp"
            tools:ignore="VectorDrawableCompat" />

        <TextView
            android:id="@+id/each_center_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginEnd="8dp"
            android:singleLine="true"
            android:text="Name"
            android:textColor="@color/text_primary_dark"
            android:textSize="@dimen/larger_text"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="@+id/each_center_intr"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toEndOf="@+id/imageView_profile_picture"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView

            android:id="@+id/each_center_time"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="8dp"
            android:layout_marginBottom="8dp"
            android:text="21"
            android:textColor="@color/accent_darker"
            android:textSize="@dimen/body1_text_size"
            app:layout_constraintBottom_toTopOf="@+id/each_center_intr"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toEndOf="@+id/each_center_name"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView

            android:id="@+id/each_center_intr"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:ellipsize="end"
            android:maxLines="1"
            android:singleLine="true"
            android:text="@string/intro"
            android:textColor="@color/text_primary_dark"
            android:textSize="@dimen/body1_text_size"
            app:layout_constraintStart_toEndOf="@+id/imageView_profile_picture"
            app:layout_constraintTop_toBottomOf="@+id/each_center_name" />

        <View
            android:id="@+id/view"
            style="@style/AppTheme.VerticalViewDivider"
            android:layout_width="1dp"
            android:layout_height="0dp"
            android:layout_marginStart="8dp"
            app:layout_constraintBottom_toBottomOf="@+id/imageView_profile_picture"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/imageView_profile_picture"
            app:layout_constraintTop_toTopOf="@+id/each_center_intro"
            app:layout_constraintVertical_bias="1.0" />

        <TextView

            android:id="@+id/each_center_intro"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_marginStart="8dp"
            android:layout_marginEnd="8dp"
            android:textColor="@color/text_secondary_dark"
            android:textSize="@dimen/body1_text_size"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="@+id/each_center_intr"
            app:layout_constraintEnd_toStartOf="@+id/view"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toEndOf="@+id/each_center_intr"
            app:layout_constraintTop_toTopOf="@+id/each_center_intr"
            app:layout_constraintVertical_bias="0.0"
            tools:text="0" />

        <TextView


            android:id="@+id/each_center_one_day"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginBottom="8dp"
            android:ellipsize="end"
            android:maxLines="1"
            android:singleLine="true"
            android:text="@string/one_day_seminar"
            android:textColor="@color/text_primary_dark"
            android:textSize="@dimen/body1_text_size"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@+id/imageView_profile_picture"
            app:layout_constraintTop_toBottomOf="@+id/each_center_intr"
            app:layout_constraintVertical_bias="0.125" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:text="@string/two_day_seminar"
            android:textColor="@color/text_primary_dark"
            android:textSize="@dimen/body1_text_size"
            app:layout_constraintStart_toEndOf="@+id/imageView_profile_picture"
            app:layout_constraintTop_toBottomOf="@+id/each_center_one_day" />

        <TextView
            android:id="@+id/each_center_one_day_edittext"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginEnd="8dp"
            android:text="0"
            android:textColor="@color/text_secondary_dark"
            android:textSize="@dimen/body1_text_size"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="@+id/each_center_one_day"
            app:layout_constraintEnd_toStartOf="@+id/view"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toEndOf="@+id/each_center_one_day"
            app:layout_constraintTop_toTopOf="@+id/each_center_one_day"
            app:layout_constraintVertical_bias="0.0" />

        <TextView
            android:id="@+id/each_center_two_day"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="8dp"
            android:text="0"
            android:textColor="@color/text_secondary_dark"
            android:textSize="@dimen/body1_text_size"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="@+id/textView7"
            app:layout_constraintEnd_toStartOf="@+id/view"
            app:layout_constraintTop_toTopOf="@+id/textView7" />

        <TextView
            android:id="@+id/each_center_21-d"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:text="21-day WS"
            android:textColor="@color/text_primary_dark"
            android:textSize="@dimen/body1_text_size"
            app:layout_constraintBottom_toBottomOf="@+id/each_center_intro"
            app:layout_constraintStart_toEndOf="@+id/view"
            app:layout_constraintTop_toTopOf="@+id/each_center_intro" />

        <TextView
            android:id="@+id/each_center_21_day"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginEnd="8dp"
            android:text="0"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="@+id/each_center_21-d"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toEndOf="@+id/each_center_21-d"
            app:layout_constraintTop_toTopOf="@+id/each_center_21-d"
            app:layout_constraintVertical_bias="0.0" />

        <TextView
            android:id="@+id/textView18"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:text="NWET"
            android:textColor="@color/text_primary_dark"
            app:layout_constraintBottom_toBottomOf="@+id/each_center_one_day_edittext"
            app:layout_constraintStart_toEndOf="@+id/view"
            app:layout_constraintTop_toTopOf="@+id/each_center_one_day_edittext" />

        <TextView
            android:id="@+id/each_center_nwet"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="8dp"
            android:textColor="@color/text_secondary_dark"
            android:textSize="@dimen/body1_text_size"
            android:textStyle="bold"
            android:text="0"
            app:layout_constraintBottom_toBottomOf="@+id/textView18"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="@+id/textView18" />

        <TextView
            android:id="@+id/textView21"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:text="mmbk(min)"
            android:textColor="@color/text_primary_dark"
            app:layout_constraintBottom_toBottomOf="@+id/each_center_two_day"
            app:layout_constraintStart_toEndOf="@+id/view"
            app:layout_constraintTop_toTopOf="@+id/each_center_two_day" />

        <TextView
            android:id="@+id/each_center_mmbk"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="8dp"
            android:textColor="@color/text_secondary_dark"
            android:textSize="@dimen/body1_text_size"
            android:textStyle="bold"
            android:text="0"
            app:layout_constraintBottom_toBottomOf="@+id/textView21"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="@+id/textView21" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.cardview.widget.CardView>