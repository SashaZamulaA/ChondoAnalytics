<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/cardView2"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginStart="8dp"
    android:layout_marginTop="2dp"
    android:layout_marginBottom="2dp"
    android:elevation="2dp"
    android:layout_marginEnd="8dp"
    app:cardBackgroundColor="@color/white">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ImageView
            android:id="@+id/imageView_profile_picture"
            android:layout_width="86dp"
            android:layout_height="86dp"
            android:layout_marginStart="8dp"
            android:layout_marginTop="8dp"
            android:layout_marginBottom="8dp"
            android:padding="@dimen/space_8"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:srcCompat="@drawable/ic_account_circle_black_24dp"
            tools:ignore="VectorDrawableCompat" />

        <TextView
            android:id="@+id/best_result_name"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginEnd="8dp"
            android:singleLine="true"
            android:text="Name"
            android:textColor="@color/text_primary_dark"
            android:textSize="@dimen/larger_text"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="@+id/each_center_intr"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toEndOf="@+id/imageView_profile_picture"
            app:layout_constraintTop_toTopOf="@+id/imageView_profile_picture" />

        <TextView

            android:id="@+id/each_center_intr"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:layout_marginTop="12dp"
            android:ellipsize="end"
            android:maxLines="1"
            android:singleLine="true"
            android:text="@string/intro"
            android:textColor="@color/text_primary_dark"
            android:textSize="@dimen/body1_text_size"
            app:layout_constraintStart_toEndOf="@+id/imageView_profile_picture"
            app:layout_constraintTop_toBottomOf="@+id/best_result_name" />

        <TextView

            android:id="@+id/best_res_intro"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_alignParentEnd="true"
            android:layout_marginTop="8dp"
            android:textColor="@color/text_secondary_dark"
            android:textSize="@dimen/body1_text_size"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="@+id/each_center_intr"
            app:layout_constraintStart_toStartOf="@+id/each_center_intr"
            app:layout_constraintTop_toBottomOf="@+id/each_center_intr"
            tools:text="0" />

        <TextView


            android:id="@+id/each_center_one_day"
            android:layout_width="wrap_content"
            android:layout_height="0dp"
            android:layout_marginStart="16dp"
            android:ellipsize="end"
            android:maxLines="1"
            android:singleLine="true"
            android:text="1-d"
            android:textColor="@color/text_primary_dark"
            android:textSize="@dimen/body1_text_size"
            app:layout_constraintBottom_toBottomOf="@+id/each_center_intr"
            app:layout_constraintStart_toEndOf="@+id/each_center_intr"
            app:layout_constraintTop_toTopOf="@+id/each_center_intr" />

        <TextView
            android:id="@+id/textView7"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:text="2-d"
            android:textColor="@color/text_primary_dark"
            android:textSize="@dimen/body1_text_size"
            app:layout_constraintBottom_toBottomOf="@+id/each_center_one_day"
            app:layout_constraintStart_toEndOf="@+id/each_center_one_day"
            app:layout_constraintTop_toTopOf="@+id/each_center_one_day" />

        <TextView
            android:id="@+id/best_result_one_day"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:text="0"
            android:textColor="@color/text_secondary_dark"
            android:textSize="@dimen/body1_text_size"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="@+id/each_center_one_day"
            app:layout_constraintStart_toStartOf="@+id/each_center_one_day"
            app:layout_constraintTop_toBottomOf="@+id/each_center_one_day" />

        <TextView
            android:id="@+id/best_res_two_day"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:text="0"
            android:textColor="@color/text_secondary_dark"
            android:textSize="@dimen/body1_text_size"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="@+id/textView7"
            app:layout_constraintStart_toStartOf="@+id/textView7"
            app:layout_constraintTop_toBottomOf="@+id/textView7" />

        <TextView
            android:id="@+id/best_res_twoOne_day"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:text="0"
            android:textColor="@color/text_secondary_dark"
            android:textSize="@dimen/body1_text_size"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="@+id/textView21"
            app:layout_constraintStart_toStartOf="@+id/textView21"
            app:layout_constraintTop_toBottomOf="@+id/textView21" />

        <TextView
            android:id="@+id/best_res_nwet"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="8dp"
            android:text="0"
            android:textColor="@color/text_secondary_dark"
            android:textSize="@dimen/body1_text_size"
            android:textStyle="bold"
            app:layout_constraintEnd_toEndOf="@+id/each_center_21-d"
            app:layout_constraintStart_toStartOf="@+id/each_center_21-d"
            app:layout_constraintTop_toBottomOf="@+id/each_center_21-d" />

        <TextView
            android:id="@+id/each_center_21-d"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:text="@string/nwet"
            android:textColor="@color/text_primary_dark"
            android:textSize="@dimen/body1_text_size"
            app:layout_constraintBottom_toBottomOf="@+id/textView21"
            app:layout_constraintStart_toEndOf="@+id/textView21"
            app:layout_constraintTop_toTopOf="@+id/textView21" />

        <TextView
            android:id="@+id/best_result"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="8dp"
            android:layout_marginEnd="8dp"
            android:text="0"
            android:textStyle="bold"
            app:layout_constraintBottom_toBottomOf="@+id/each_center_21-d"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toEndOf="@+id/each_center_21-d"
            app:layout_constraintTop_toTopOf="@+id/each_center_21-d"
            app:layout_constraintVertical_bias="0.0" />

        <TextView
            android:id="@+id/textView21"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:text="21-d"
            android:textColor="@color/text_primary_dark"
            app:layout_constraintBottom_toBottomOf="@+id/textView7"
            app:layout_constraintStart_toEndOf="@+id/textView7"
            app:layout_constraintTop_toTopOf="@+id/textView7" />

    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.cardview.widget.CardView>